{
  "name": "neurosongs-2",
  "private": true,
  "description": "Source code for Neurosongs 2",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean-update": "turbo run clean-update && npm-check-updates -u",
    "dev": "turbo run dev",
    "format": "npm run format-package-json && npm run format-workspaces",
    "format-package-json": "eslint --fix --suppress-all \"./**/package.json\" && rm -f eslint-suppressions.json",
    "format-workspaces": "turbo run format",
    "generate": "turbo run generate",
    "generate-from-back-end": "turbo run generate --filter=back-end",
    "lint": "npm run lint-package-json && npm run lint-workspaces",
    "lint-package-json": "eslint \"./**/package.json\"",
    "lint-workspaces": "turbo run lint",
    "migrate-deploy": "turbo run migrate-deploy --filter=back-end",
    "prepare": "husky",
    "test": "turbo run test",
    "update-dependencies": "turbo run clean-update && npm run update-root-dependencies",
    "update-root-dependencies": "bash -c 'npx npm-check-updates -u \"$@\" && npm install --workspaces' --"
  },
  "dependencies": {
    "@alextheman/utility": "^1.19.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@alextheman/eslint-plugin": "^1.14.2",
    "@types/node": "^24.6.2",
    "eslint": "^9.37.0",
    "husky": "^9.1.7",
    "npm-check-updates": "^19.0.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.3"
  },
  "packageManager": "npm@11.6.1",
  "engines": {
    "node": ">=18"
  }
}
