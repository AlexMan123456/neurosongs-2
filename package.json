{
  "name": "neurosongs-2",
  "private": true,
  "description": "Source code for Neurosongs 2",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean-update": "turbo run clean-update && bash ./clean-update.sh",
    "dev": "turbo run dev",
    "format": "eslint --fix --suppress-all \"./**/package.json\" && rm -f eslint-suppressions.json && turbo run format",
    "generate": "turbo run generate",
    "generate-from-back-end": "turbo run generate --filter=back-end",
    "lint": "eslint \"./**/package.json\" && turbo run lint",
    "migrate-deploy": "turbo run migrate-deploy --filter=back-end",
    "prepare": "husky",
    "test": "turbo run test",
    "update-dependencies": "turbo run update-dependencies && npm run update-root-dependencies",
    "update-root-dependencies": "bash -c 'npx npm-check-updates -u \"$@\" && npm install' --"
  },
  "dependencies": {
    "@alextheman/utility": "^1.16.0"
  },
  "devDependencies": {
    "@alextheman/eslint-plugin": "^1.13.6",
    "@types/node": "^24.3.1",
    "eslint": "^9.35.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "npm@11.6.0",
  "engines": {
    "node": ">=18"
  }
}
