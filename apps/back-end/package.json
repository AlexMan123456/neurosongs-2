{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Back-end of Neurosongs 2",
  "license": "ISC",
  "author": "alextheman",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "clean-update": "bash ../../clean-update.sh",
    "dev": "nodemon src/server/index.ts",
    "format": "prettier --write --parser typescript \"src/**/*.ts\" \"tests/**/*.ts\" && eslint --fix --suppress-all \"src/**/*.ts\" \"tests/**/*.ts\" && rm -f eslint-suppressions.json",
    "generate": "npx prisma generate && npx tsx disableTscInDirectory.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts\" \"tests/**/*.ts\" && prettier --check --parser typescript \"src/**/*.ts\" \"tests/**/*.ts\"",
    "migrate-deploy": "prisma migrate deploy",
    "migrate-dev": "prisma migrate dev",
    "reset-database": "npx prisma migrate reset --force --skip-generate",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test-watch": "vitest",
    "update-dependencies": "bash -c 'npx npm-check-updates -u \"$@\" && npm install' --"
  },
  "dependencies": {
    "@neurosongs/prisma-client": "^1.0.0",
    "@neurosongs/types": "^1.0.0",
    "@neurosongs/utility": "^1.0.0",
    "@prisma/client": "^6.16.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@chax-at/transactional-prisma-testing": "^1.4.0",
    "@types/express": "^5.0.3",
    "@types/supertest": "^6.0.3",
    "execa": "^9.6.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "tsup": "^8.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
