{
  "name": "front-end",
  "version": "0.0.0",
  "private": true,
  "description": "Front-end of Neurosongs 2",
  "author": "alextheman",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "clean-update": "bash -c 'npx npm-check-updates -u \"$@\"'",
    "dev": "vite",
    "format": "npm run format-prettier && npm run format-eslint",
    "format-eslint": "eslint --fix --suppress-all \"src/**/*.{ts,tsx}\" && rm -f eslint-suppressions.json",
    "format-prettier": "npm run format-prettier-typescript && npm run format-prettier-javascript",
    "format-prettier-javascript": "prettier --write \"./**/*.js\"",
    "format-prettier-typescript": "prettier --write --parser typescript \"./**/*.{ts,tsx}\"",
    "lint": "npm run lint-tsc && npm run lint-eslint && npm run lint-file-dependencies && npm run lint-prettier",
    "lint-eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-file-dependencies": "npx alex-c-line check-for-file-dependencies",
    "lint-prettier": "npm run lint-prettier-typescript && npm run lint-prettier-javascript",
    "lint-prettier-javascript": "prettier --check \"./**/*.js\"",
    "lint-prettier-typescript": "prettier --check --parser typescript \"./**/*.{ts,tsx}\"",
    "lint-tsc": "tsc --noEmit",
    "preview": "vite preview",
    "update-dependencies": "bash -c 'npx npm-check-updates -u \"$@\" && npm install' --",
    "use-live-components": "npm uninstall @alextheman/components && npm install @alextheman/components",
    "use-local-components": "dotenv -e .env -- sh -c 'COMPONENTS_PATH=${LOCAL_COMPONENTS_PATH:-../../../components}; npm --prefix \"$COMPONENTS_PATH\" run create-local-package && npm uninstall @alextheman/components && npm install \"$COMPONENTS_PATH\"/alextheman-components-*.tgz'"
  },
  "dependencies": {
    "@alextheman/components": "^5.8.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@neurosongs/types": "^1.0.0",
    "@tanstack/react-query": "^5.90.10",
    "axios": "^1.13.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "dotenv-cli": "^11.0.0",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
