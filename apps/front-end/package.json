{
  "name": "front-end",
  "version": "0.0.0",
  "private": true,
  "description": "Front-end of Neurosongs 2",
  "author": "alextheman",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format": "pnpm run format-prettier && pnpm run format-eslint",
    "format-eslint": "eslint --fix --suppress-all \"src/**/*.{ts,tsx}\" && rm -f eslint-suppressions.json",
    "format-prettier": "pnpm run format-prettier-typescript && pnpm run format-prettier-javascript",
    "format-prettier-javascript": "prettier --write \"./**/*.js\"",
    "format-prettier-typescript": "prettier --write --parser typescript \"./**/*.{ts,tsx}\"",
    "lint": "pnpm run lint-tsc && pnpm run lint-eslint && pnpm run lint-file-dependencies && pnpm run lint-prettier",
    "lint-eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-file-dependencies": "alex-c-line check-for-file-dependencies",
    "lint-prettier": "pnpm run lint-prettier-typescript && pnpm run lint-prettier-javascript",
    "lint-prettier-javascript": "prettier --check \"./**/*.js\"",
    "lint-prettier-typescript": "prettier --check --parser typescript \"./**/*.{ts,tsx}\"",
    "lint-tsc": "tsc --noEmit",
    "preview": "vite preview",
    "update-dependencies": "pnpm update --latest && pnpm update",
    "use-live-components": "pnpm uninstall @alextheman/components && pnpm install @alextheman/components",
    "use-local-components": "dotenv -e .env -- sh -c 'COMPONENTS_PATH=${LOCAL_COMPONENTS_PATH:-../../../components}; pnpm --prefix \"$COMPONENTS_PATH\" run create-local-package && pnpm uninstall @alextheman/components && pnpm install \"$COMPONENTS_PATH\"/alextheman-components-*.tgz'"
  },
  "dependencies": {
    "@alextheman/components": "^6.1.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@neurosongs/types": "link:../../packages/types",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.13.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "alex-c-line": "^1.6.1",
    "dotenv-cli": "^11.0.0",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.6"
  }
}
