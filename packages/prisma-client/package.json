{
  "name": "@neurosongs/prisma-client",
  "version": "1.0.0",
  "description": "Prisma Client for use in Neurosongs back-end",
  "license": "ISC",
  "author": "alextheman",
  "type": "module",
  "exports": {
    "./fabbrica": {
      "types": "./src/fabbrica.ts",
      "import": "./dist/fabbrica.js",
      "require": "./dist/fabbrica.cjs",
      "default": "./dist/fabbrica.js"
    },
    "./prisma": {
      "types": "./src/prisma.ts",
      "import": "./dist/prisma.js",
      "require": "./dist/prisma.cjs",
      "default": "./dist/prisma.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "default": "./dist/types.js"
    }
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/generated && pnpm run generate && tsdown && cp -r src/generated dist/generated",
    "format": "prisma format",
    "generate": "prisma generate && npx tsx disableTscInDirectory.ts",
    "lint": "prisma format --check",
    "update-dependencies": "pnpm update --latest && pnpm update"
  },
  "dependencies": {
    "@neurosongs/utility": "workspace:../utility",
    "@types/node": "^24.10.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "@quramy/prisma-fabbrica": "2.3.2",
    "prisma": "^6.19.0",
    "prisma-zod-generator": "1.32.1",
    "tsdown": "^0.17.0"
  },
  "peerDependencies": {
    "@prisma/client": "^6.19.0",
    "@quramy/prisma-fabbrica": "^2.3.2",
    "prisma-zod-generator": "^1.32.1"
  }
}
