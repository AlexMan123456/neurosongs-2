{
  "name": "@neurosongs/prisma-client",
  "version": "1.0.0",
  "description": "Prisma Client for use in Neurosongs back-end",
  "license": "ISC",
  "author": "alextheman",
  "type": "module",
  "exports": {
    "./fabbrica": {
      "types": "./src/fabbrica.ts",
      "import": "./dist/fabbrica.js",
      "require": "./dist/fabbrica.cjs",
      "default": "./dist/fabbrica.js"
    },
    "./prisma": {
      "types": "./src/prisma.ts",
      "import": "./dist/prisma.js",
      "require": "./dist/prisma.cjs",
      "default": "./dist/prisma.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "default": "./dist/types.js"
    }
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/generated && npm run generate && tsup && cp -r src/generated dist/generated",
    "clean-update": "bash -c 'npx npm-check-updates -u \"$@\"'",
    "format": "prisma format",
    "generate": "prisma generate && npx tsx disableTscInDirectory.ts",
    "lint": "prisma format --check",
    "update-dependencies": "bash -c 'npx npm-check-updates -u \"$@\" && npm install' --"
  },
  "dependencies": {
    "@neurosongs/utility": "^1.0.0",
    "@prisma/client": "^6.18.0",
    "@types/node": "^24.9.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@quramy/prisma-fabbrica": "^2.3.1",
    "prisma": "^6.18.0",
    "prisma-zod-generator": "^1.30.2",
    "tsup": "^8.5.0"
  }
}
