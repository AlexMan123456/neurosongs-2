{
  "name": "@neurosongs/prisma-client",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "generate": "prisma generate",
    "update-dependencies": "npx npm-check-updates -u && npm install",
    "format": "prettier --write --parser typescript 'src/**/*.ts' 'prisma.config.ts' 'fix-generated-prisma-imports.ts' && ESLINT_MODE=fix eslint --fix 'src/**/*.ts' 'prisma.config.ts' 'fix-generated-prisma-imports.ts'",
    "lint": "ESLINT_MODE=lint eslint 'src/**/*.ts' 'prisma.config.ts' 'fix-generated-prisma-imports.ts' && prettier --check --parser typescript 'src/**/*.ts' 'prisma.config.ts' 'fix-generated-prisma-imports.ts'",
    "build": "rm -rf dist/generated && prisma generate && tsup && cp -r generated dist/generated && npx tsx fix-generated-prisma-imports.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "prisma": "^6.14.0",
    "tsup": "^8.5.0"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/node": "^24.3.0"
  }
}
