{
  "name": "@neurosongs/prisma-client",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "exports": {
    "./prisma": {
      "types": "./src/prisma.ts",
      "import": "./dist/prisma.js",
      "require": "./dist/prisma.cjs",
      "default": "./dist/prisma.js"
    },
    "./fabbrica": {
      "types": "./src/fabbrica.ts",
      "import": "./dist/fabbrica.js",
      "require": "./dist/fabbrica.cjs",
      "default": "./dist/fabbrica.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "default": "./dist/types.js"
    }
  },
  "scripts": {
    "generate": "prisma generate",
    "update-dependencies": "bash -c 'npx npm-check-updates -u \"$@\" && npm install' --",
    "build": "rm -rf dist/generated && prisma generate && tsup && cp -r src/generated dist/generated",
    "format": "prisma format && prettier --write --parser typescript \"src/types.ts\" && eslint --fix --suppress-all \"src/types.ts\" && rm -f eslint-suppressions.json",
    "lint": "prisma format --check && eslint \"src/types.ts\" && prettier --check --parser typescript \"src/types.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@quramy/prisma-fabbrica": "^2.3.1",
    "prisma": "^6.15.0",
    "prisma-zod-generator": "^1.17.1",
    "tsup": "^8.5.0"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@types/node": "^24.3.0",
    "zod": "^4.1.4"
  }
}
